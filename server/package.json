    "db:seed": "node dist/scripts/seed.js",
    "db:test": "node test-db.js",
    "setup": "node setup.js",
    "create-academy": "ts-node src/scripts/createNewAcademy.ts",
    "setup-sqlite": "ts-node src/scripts/setup-sqlite.ts",
